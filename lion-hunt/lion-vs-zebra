const input1 = '"LZ"';
const input2 = '"Z L"';
const input3 = '"L     Z"';
const input4 = '"L  L"';
const input5 = '"Z  Z  Z"';
const input6 = '"L ZL Z"';

console.log(input1,'        0');
console.log(input2,'       1');
console.log(input3,'   5');
console.log(input4,'     -1');
console.log(input5,'  -1');
console.log(input6,'    0');
